{
  "name": "freecodecamp-news",
  "description": "A place to host articles about software and technology",
  "version": "0.0.0",
  "author": "freeCodeCamp",
  "dependencies": {
    "axios": "^0.17.1",
    "chalk": "^2.3.0",
    "date-fns": "^1.29.0",
    "gatsby": "^1.9.110",
    "gatsby-link": "^1.6.27",
    "gatsby-plugin-less": "^1.0.6",
    "gatsby-plugin-react-helmet": "^1.0.8",
    "gatsby-source-filesystem": "^1.5.8",
    "gatsby-transformer-remark": "^1.7.21",
    "lodash": "^4.17.4",
    "puppeteer": "^0.13.0",
    "react-bootstrap": "^0.31.5",
    "react-fontawesome": "^1.6.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-create-types": "^0.0.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-observable": "^0.17.0",
    "rxjs": "^5.5.2",
    "slugg": "^1.2.1",
    "store": "^2.0.12",
    "to-markdown": "^3.1.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "BSD-3-Clause",
  "main": "n/a",
  "scripts": {
    "build": "yarn slugsafe && node --max-old-space-size=7168 ./node_modules/gatsby/dist/bin/gatsby build",
    "dev": "yarn slugsafe && node --max-old-space-size=7168 ./node_modules/gatsby/dist/bin/gatsby develop",
    "format": "yarn format-js && yarn format-jsx",
    "format-js": "prettier es5 --single-quote --write './**/*.js'",
    "format-jsx": "prettier es5 --single-quote --write './**/*.jsx'",
    "lint": "eslint --ext .js,.jsx --ignore-path .gitignore . --fix",
    "slugsafe": "node slugSafe.js",
    "test": "yarn slugsafe && yarn format && yarn lint"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "eslint": "^4.10.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.4.0",
    "prettier": "^1.8.2"
  }
}
